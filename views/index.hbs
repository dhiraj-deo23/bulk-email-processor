<div class="welcome">
  <h4 class="welcome-text">Welcome {{username}}</h4>
</div>
<div class="row">
  <div class="col s12 m8 offset-m2">
    {{#each emailTemplates}}

      <div class="card templates">
        <div class="card-content center-align">
          <p><b>subject:</b> {{subject}}</p>
          <p><b>body:</b> {{text}}</p>
          {{html}}
          <br />
          <br />
          <a href="/email/{{_id}}" class="btn">Select</a>
        </div>
      </div>
    {{/each}}

  </div>
</div>