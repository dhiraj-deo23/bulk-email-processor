<div class="welcome">
  <h4 class="welcome-text">Welcome {{username}}</h4>
</div>
<div class="row">
  <div class="col s12 m8 offset-m2">
    <input type="hidden" name="id" id="idd" value="{{id}}" />

    <div class="card">
      <div class="card-content center-align">
        <form action="" method="post" enctype="multipart/form-data">
          <input type="file" name="excel" id="" />
          <br /><br />
          <input type="submit" value="Upload" class="btn" />
        </form>
        {{#if error}}
          <p>{{error}}</p>
        {{/if}}
      </div>
    </div>

    <h4 class="text blue center" id="upload">Logs</h4>

  </div>
</div>